<!DOCTYPE html>
<html lang="en">
<head>
  <title>To Do List App</title>
  <link rel="stylesheet" href="/css/style.css">
  <link rel="stylesheet" href="https://stackpath.bootstrapcdn.com/bootstrap/4.4.1/css/bootstrap.min.css" integrity="sha384-Vkoo8x4CGsO3+Hhxv8T/Q5PaXtkKtu6ug5TOeNV6gBiFeWPGFN9MuhOf23Q9Ifjh" crossorigin="anonymous">
</head>
<body>
  <div class="box border">
    <table class="table table-borderless">
      <thead>
        <th><h1>User List</h1></th>
        <th><a href="/admin/delete" class="btn btn-danger">-</a></th>
        <th><a href="/admin/new" class="btn btn-success">+</a></th>
        <th><a href="/admin/profile" class="btn btn-dark">Profile</a></th>
        <th><a href="/logout" class="btn btn-info">Logout</a></th>
      </thead>
      <tbody>
        <% for(var i=0; i<activityList.length; i++) { %>
          <tr>
            <td>- <%= activityList[i].email %></td>
            <td><a href="/admin/update/<%= activityList[i].user_id %>" class="btn btn-warning">...</a></td>
          </tr>    
        <% } %>
      </tbody>
     </table>
  </div>
</body>
</html>